<script>
	function showAgree() {
		if( $(".emailAgree").css("visibility") == "hidden") {
			$('.emailAgree').css("visibility","visible");
			$('.emailAgree').animate({ opacity: 1 }, { duration: 500 });
			$("#boverlay2").animate({ opacity: 0 }, { duration: 0 });
			$("#boverlay2").show();
			$("#boverlay2").animate({ opacity: 0.8 }, { duration: 500 });
		} else {
			$('.emailAgree').animate({ opacity: 0 }, { duration: 300, complete: function () { $('.emailAgree').css("visibility","hidden"); } });
			$("#boverlay2").animate({ opacity: 0 }, { duration: 500, complete: function () { $("#boverlay2").hide(); } });
		}
	}


	function goScroll(obj) {
		$('html, body').stop().animate({ 'scrollTop': obj.offset().top }, 400, 'swing');
	}

</script>

